@page
@model DraftProspectsFrontEnd.Pages.NCAA.NCAAPPGModel
@{
    ViewData["Title"] = "NCAA PPG AVG Ranking";
}

<h1>@ViewData["Title"]</h1>

<table class="table-bordered">
    <thead>
        <tr class="table-active">
            <th>Rank</th>
            <th>Name    </th>
            <th>PPG Avg</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var player in Model.GetNCAAPPGAvg)
        {
                 <tr>
                        <td>@player.Rank </td>
                        <td>@player.Name </td>
                        <td>@player.PPGAvg </td>
                 </tr>
        }
    </tbody>
</table>
